{
  "name": "back",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "npm run doc:generate && npm run dev",
    "start:prod": "npm run build:clean && npm run build && npm run prod",
    "dev": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
    "prod": "cross-env NODE_ENV=production node --enable-source-maps dist/index.js",
    "build": "tsc --project tsconfig.build.json && npm run build:postprocess",
    "build:clean": "rm -r dist; exit 0",
    "doc:generate": "swagger-cli bundle src/app/swagger/index.yaml --outfile src/app/swagger/swagger.json",
    "doc:validate": "swagger-cli validate src/app/swagger/index.yaml",
    "lint": "eslint --fix",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "glob": "^11.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "swagger-cli": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.23.0",
    "yamljs": "^0.3.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
